{
  "name": "table-relation-analyzor",
  "version": "1.0.0",
  "engines": {
    "node": ">=v10.8.0",
    "npm": ">=6.3.0"
  },
  "description": "to visualize the depedency of table from DDL",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc mocha --opts ./spec/mocha.opts --recursive './spec/**/*.spec.js'",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "npm run build && node dist/index.js",
    "eslint": "eslint ./src ./spec",
    "cover": "npm test && node_modules/.bin/nyc report --reporter=text-lcov | coveralls",
    "check-coverage": "nyc check-coverage --lines 100 --per-file"
  },
  "keywords": [
    "visualize",
    "table"
  ],
  "author": "victoryw",
  "license": "MIT",
  "dependencies": {
    "graphviz": "^0.0.8",
    "line-reader": "^0.4.0",
    "ramda": "^0.25.0",
    "single-line-log": "^1.1.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-istanbul": "5.2.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "coveralls": "3.0.2",
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-mocha": "5.2.0",
    "js-yaml": "3.12.0",
    "mocha": "5.2.0",
    "mocha-lcov-reporter": "1.3.0",
    "nyc": "13.0.1",
    "rimraf": "2.6.2"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false,
    "exclude": "spec/**/*.js"
  }
}
